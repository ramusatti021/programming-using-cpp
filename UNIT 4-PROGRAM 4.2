#include<iostream>
#include<cstdlib>
using namespace std;
class DispDemo
{
 public:
 void display()
 {
 cout << "Nothing " << endl;
 }
 void display(string msg)
 {
 cout << "Hello Everone : " << msg << endl;
 }
 void display(int age)
 {
 cout << "Age : " <<age << endl;
 }
 void display(string msg,int age)
 {
 cout << "Hello " << msg<<"\n" <<"Age:" <<age << endl;
 }
 int display(int age, int grace)
 {
 return (age+grace);
 }
 void show(int c,int a = 10,int b = 20)
 {
 cout << "A:" << a <<" B:" <<b << " C:" << c<< endl;
 }
};
int main()
{
 DispDemo dd;
 dd.display(40);
 dd.display();
 dd.display("Welcome to C++ programming");
 dd.display("Ramu",20);
 cout << "Result:" << dd.display(30, 6) << endl;
 dd.show(50);
 dd.show(10, 20);
 return 0;
}
