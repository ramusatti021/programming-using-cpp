#include<iostream>
#include<cstdlib>
using namespace std;
class DispDemo
{
 public:
 void display()
 {
 cout<<"Nothing to say"<<endl;
 }
 void display(string msg)
 {
 cout<<"Say :"<<msg<<endl;
 }
 void display(int age)
 {
 cout<<"Age: "<<age<<endl;
 }
 void display(string msg,int age)
 {
 cout<<"Hello "<<msg<<endl;
 cout<<"Age :"<<age<<endl;
 }
 int display(int age, int grace)
 {
 return (age+grace);
 }
};
int main()
{
 DispDemo dd;
 dd.display(30);
 dd.display();
 dd.display("Welcome to c++ programming");
 dd.display("Ramu ",19);
 cout<<"result:"<<dd.display(19,1);
 return 0;
} 
